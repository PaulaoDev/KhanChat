  "main": "./app/index.js",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run http-server-local\" \"npm run webpack-dev\"",
    "http-server-local": "nodemon --exec \"node -r dotenv/config\" ./app/index.js dotenv_config_path=env/local/.env",
    "http-server-prod": "node -r dotenv/config ./app/index.js dotenv_config_path=env/prod/.env",
    "gulp": "gulp",
    "webpack-dev": "webpack --progress -p --config webpack.config.js",
    "start:dev": "npm run dev",
    "start": "npm run http-server-prod",
    "build": "node build/build.js"
  },
